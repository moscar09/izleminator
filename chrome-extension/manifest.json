{
  "manifest_version": 2,

  "name": "Izleminator",
  "description": "Enables a chat client along your video stream",
  "version": "0.4",
  "content_scripts": [{
    "matches": [
        "http://tatooine.moscar.ro/*",
        "http://dizimag2.co/*",
        "http://dizimag3.co/*"
    ],
    "js": [
        "scripts/lib/jquery.min.js",
        "scripts/config.js",
        "scripts/playerWrappers/JWPlayerWrapper.js",
        "scripts/playerWrappers/test-player-wrapper.js",
        "scripts/izleminator-client.js",
        "scripts/izleminator-chat.js",
        "scripts/main.js"
    ],
    "css": ["style.css"],
    "run_at":"document_end"
  }],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Izleminate!"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    "scripts/playerWrappers/JWPlayerWrapper.js",
    "scripts/playerWrappers/test-player-wrapper.js",
    "scripts/lib/jquery.min.js",    
    "scripts/inlined.js"
  ],
  "externally_connectable": {
    "matches": [
        "http://tatooine.moscar.ro/*",
        "http://dizimag2.co/*",
        "http://dizimag3.co/*"
    ]
  }
}
